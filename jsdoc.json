{
  "source": {
    "include": ["js"],
    "exclude": ["./js/buzz.min.js", "./js/jquery-3.1.1.min.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
